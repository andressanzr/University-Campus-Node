<div class="jumbotron">
    <h1 class="display-4"><%= subject.nombre %></h1>
    <h4 class="display-4"><%= subject.tipoPlanEstudio %></h1>
        <p class="lead">Profesor:
            <% if(subject.profResponsables){ %>
            <% subject.profResponsables.map(profe =>{ %>
            <h5>
                <%= profe.nombre  %>
                <%= profe.apellido  %>
            </h5>
            <%  }) %>
            <% } %>
        </p>
        <p class="lead">Alumnos:
            <% if(subject.alumnos){ %>
            <% subject.alumnos.map(alumno =>{ %>
            <h5>
                <%= alumno.nombre %>
                <%= alumno.apellido %>
            </h5>
            <%  }) %>
            <% } %></p>
        <hr class="my-4">
        <p><%= subject.infoInstalar %></p>
        <p class="lead">
            <ul>
                <% if(subject.enlaces){ %>
                <% subject.enlaces.map( enlace =>{ %>
                <li><a href="<%= enlace %>"><%= enlace %></a></li>
                <% }) } %>
            </ul>
        </p>
        <% for(var x=0; x< filesList.length; x++ ){ %>
        <% if(filesList[x].subjId == subject._id){ %>
        <% for(var y=0; y < filesList[x].files.length; y++ ){ %>
        <img width="50px" src="/images/fileIcon.png" />
        <p><a href="<%= './files/subjectFiles/'+ subject._id + '/' + filesList[x].files[y] %>"
                class=""><%= filesList[x].files[y] %></a>
        </p>
        <% } %>
        <% } %>
        <% } %>
</div>